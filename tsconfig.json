{
    "compilerOptions": {
        "target": "ES2018",
    "module": "ESNext",
    "resolveJsonModule": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
      "types": [
        "buefy/types"
      ]
    },
    "exclude": ["node_modules", ".nuxt", "dist"]
  }